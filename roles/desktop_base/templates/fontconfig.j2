<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE fontconfig SYSTEM "../fonts.dtd">
<fontconfig>
  <alias>
    <family>sans-serif</family>
      <prefer>
        <family>{{ font.sans }}</family>
      </prefer>
  </alias>
  <alias>
    <family>{{ font.sans }}</family>
      <default>
        <family>sans-serif</family>
      </default>
  </alias>
  <alias>
    <family>serif</family>
      <prefer>
        <family>{{ font.serif }}</family>
      </prefer>
  </alias>
  <alias>
    <family>{{ font.serif }}</family>
      <default>
        <family>serif</family>
      </default>
  </alias>
  <alias>
    <family>monospace</family>
      <prefer>
        <family>{{ font.monospace }}</family>
      </prefer>
  </alias>
  <alias>
    <family>{{ font.monospace }}</family>
      <default>
        <family>monospace</family>
      </default>
  </alias>
</fontconfig>
