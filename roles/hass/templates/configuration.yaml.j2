homeassistant:
  name: Home
  latitude: {{ hass.latitude }}
  longitude: {{ hass.longitude }}
  time_zone: Asia/Shanghai
  unit_system: metric
  auth_providers:
    - type: homeassistant
    # - type: legacy_api_password

automation:

config:

conversation:

discovery:

frontend:

group:

history:

http:
  server_host: 127.0.0.1

logbook:

map:

media_player:
  - platform: braviatv
    host: {{ hass.braviatv_host }}

notify:
  - name: html5
    platform: html5
    gcm_api_key: '{{ hass.gcm_api_key }}'
    gcm_sender_id: '{{ hass.gcm_sender_id }}'

script:

sensor:
  - platform: yr
    monitored_conditions:
      - temperature
      - symbol
      - humidity

sun:

tts:
  - platform: google

updater:
  reporting: False
